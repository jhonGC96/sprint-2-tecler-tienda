<%- include ("cabeceras/cabecera.listar.ejs") %>
    <div class="container mt-5">
        <div class="row">
            <div class="col-md-7">
                <table class="table table-bordered table-hover">
                    <thead>
                        <tr>
                            <td>nÂ°</td>
